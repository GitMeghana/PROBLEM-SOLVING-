import java.io.*;
import java.util.*;

public class Solution {
  public static void main(String [] args){
      Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        if (n%2 == 0){
        int n1 = 0 ,n2 = 1,c;
        for (int i =0;i<n/2;i++){
            c = n1 + n2;
            n1 = n2;
            n2 = c;
        } 
        System.out.println(n1);
    }
        else {
            System.out.println(n);
        }
    }
}
